{
  "name": "my-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www --exec babel-node --presets @babel/preset-env",
    "test": "NODE_ENV=test npm run db:init && NODE_ENV=test ./node_modules/.bin/jest tests --coverage",
    "db:init": "sequelize db:drop && sequelize db:create && sequelize db:migrate",
    "migrate": "sequelize db:migrate",
    "migrate:reset": "sequelize db:migrate:undo:all",
    "seeding": "sequelize db:seed:all"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "babel-core": "^6.26.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "helmet": "^4.0.0",
    "http-errors": "~1.6.3",
    "http-status": "^1.4.2",
    "jest": "^26.2.2",
    "moment": "^2.27.0",
    "morgan": "~1.9.1",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.4",
    "prettier-eslint": "^11.0.0",
    "sequelize": "^6.3.4",
    "sequelize-cli": "^6.2.0",
    "uuid4": "^2.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "babel-jest": "^26.2.2",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5",
    "random-string": "^0.2.0",
    "regenerator-runtime": "^0.13.7",
    "supertest": "^4.0.2"
  }
}
